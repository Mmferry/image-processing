  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug image processing app",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": ["-r", "ts-node/register"],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}\\src\\index.ts",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
  ]
}